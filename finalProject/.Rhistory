## less10
summary(less10_AIC)
less10_BMindex <- round(summary(less10_AIC) $coefficients[2][1],2)
less10_GDP <- round(summary(less10_AIC) $coefficients[3][1],2)
less10_GNI <- round(summary(less10_AIC) $coefficients[4][1],2)
less10_age15_64 <- round(summary(less10_AIC) $coefficients[5][1],2)
less10_Birth <- round(summary(less10_AIC) $coefficients[6][1],2)
less10_usa_Birth <- round(summary(less10_AIC) $coefficients[7][1],2)
less10_usa_une <- round(summary(less10_AIC) $coefficients[8][1],2)
less10_Intercept <- round(summary(less10_AIC) $coefficients[1][1],2)
IQR_g <- glm(dollar_ex~
BMindex + GDP + GNI + age15_64 + Birth +
inflationByCpi + unemployment + ecoGrowthRate +
usa_GDP +usa_GNI + usa_age + usa_Birth +
usa_inflation + usa_une + usa_eco,
data = allDataIQR)
IQR_AIC <- stepAIC(IQR_g, direction = "both", trace=F)
summary(IQR_AIC)
